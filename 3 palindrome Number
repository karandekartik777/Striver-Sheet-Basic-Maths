n = int(input("please enter a number"))
ncopy=n
def palindromeNumber(n):
    reverseNum=0
    while n>0:
        lastdigit = n%10
        reverseNum = reverseNum*10+lastdigit 
        n=n//10
    if ncopy == reverseNum:
        print("yes")
    else:
        print("no")
    
    
palindromeNumber(n)
      
